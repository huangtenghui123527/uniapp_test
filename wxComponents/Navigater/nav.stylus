.nav-warp{
	padding 20upx 0;
	width 90%;
	position fixed;
	left 50%;
	bottom 20upx;
	border 1upx solid rgba(0,0,0,0.15);
	background-color #ef384a;
	box-shadow: 0 0 20upx 0 rgba(0,0,0,.22);
	transform translateX(-50%);
	border-radius 80upx;
	.btn-area{
		width 100%;
		height 50upx;
		display :flex;
		justify-content :space-between;
		align-items center;
		li{
			width 25%;
			text-align center;
			font-size 28upx;
			display flex;
			flex-direction column;
			align-items center;
			// color rgba(0,0,0,0.65);
			color rgba(255,255,255,0.85);
		}
		.shopping_mall{
			position relative;
		    top: -24upx;
		    padding: 20upx 20upx 0 20upx;
		    background: #ef384a;
		    border-radius: 50%;
		    width: 140upx;
		    height: 140upx;
			i{
				font-size 68upx;
				color #fff;
				margin-bottom: 4upx;
			}
		}
		// image{
		// 	width 20upx;
		// 	height 20upx;
		// 	margin-bottom 3upx;
		// }
		i{
			font-size 40upx;
		}
		text{
			font-size 26upx;
		}
		.current_select{
			// color rgba(255,255,255,0.85);
			color #FED69E;
		}
	}
	
}